  return 0;
}

void print_matrix(int matrix[5][5])
{
	int y, x; 
    for(y = 0; y < 5; y++) 
        {
          for(x = 0; x < 5; x++) 
          {
			  cout << matrix[y][x] << " ";
			  if (x == 4)
				  {
				  	printf("\n");	
				  }
          } 
        }
}
	
int count_sum(int matrix[5][5])
{
	int total = 0;
	int y, x; 
    for(y = 0; y < 5; y++) 
        {
          for(x = 0; x < 5; x++) 
          {
			  total = total + matrix[y][x];
          } 
        }
	return total;	
}
